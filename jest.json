{
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "testRegex": "/__tests__/.*\\.spec\\.js$",
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "watchman": false,
  "notify": true,
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "setupFiles": [
    "<rootDir>/__tests__/setup.js"
  ]
}
