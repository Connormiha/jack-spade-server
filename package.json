  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/jack-spade-server.git"
  },
  "scripts": {
    "postinstall": "flow-typed install",
    "coverage": "jest --coverage --config jest.json",
    "lint": "gulp lint",
    "precommit": "NODE_ENV=pre_commit npm run types-check && npm run lint",
    "prepush": "npm run types-check && npm test",
    "start": "npm run types-check && babel-node ./src/run.js",
    "test": "jest --no-cache --config jest.json",
    "types-check": "flow src",
    "upgrade": "yarn upgrade-interactive --exact"
  },
  "devDependencies": {
    "babel-eslint": "7.2.1",
    "eslint": "3.19.0",
    "eslint-plugin-flowtype": "2.30.4",
    "flow-bin": "0.43.1",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "husky": "0.13.3",
    "jest": "^19.0.2"
  },
  "dependencies": {
    "app-module-path": "2.2.0",
    "babel-cli": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "flow-typed": "2.0.0"
  }
